{
  "name": "cdk_template",
  "version": "0.1.0",
  "bin": {
    "cdk_template": "dist/bin/cdk_template.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "eslint \"bin/**/*.ts\" \"lib/**/*.ts\" \"test/**/*.ts\"",
    "lint-fix": "eslint --fix \"bin/**/*.ts\" \"lib/**/*.ts\" \"test/**/*.ts\"",
    "protoc": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. --proto_path=./bin/proto ./bin/proto/simple.proto"
  },
  "devDependencies": {
    "@protobuf-ts/protoc": "^2.2.2",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "aws-cdk": "2.11.0",
    "esbuild": "^0.14.21",
    "eslint": "^8.8.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "ts-proto": "^1.104.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "aws-cdk-lib": "2.11.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}
